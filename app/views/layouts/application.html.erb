<!DOCTYPE html>
<html>
  <head>
    <title>MarikenWebsite</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <script src="https://kit.fontawesome.com/03973d01eb.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="path/to/font-awesome/css/font-awesome.min.css">
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="navbar">
      <ul>
        <div class="tabs">
          <div id="home-tab"><li><%= link_to "HOME", pages_home_path %></li></div>
          <div><li><%= link_to "About me", pages_about_path %></li></div>
          <div><li><%= link_to "Projects", pages_projects_path %></li></div>
          <div><li><%= link_to "Web development", pages_web_development_path %></li></div>
          <div><li><%= link_to "Contact", pages_contact_path %></li></div>
        </div>
      </ul>
    </div>
    <%= yield %>

    <%= javascript_pack_tag "application" %>    <!-- from app/javascript/packs/application.js -->
  </body>
</html>
