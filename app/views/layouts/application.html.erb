<!DOCTYPE html>
<html>
  <head>
    <title>MarikenWebsite</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <script src="https://kit.fontawesome.com/03973d01eb.js" crossorigin="anonymous"></script>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="navbar">
      <ul>
        <div class="home-tab">
          <li><%= link_to "HOME", pages_home_path %></li>
        </div>
        <div class="tabs">
          <li><%= link_to "About me", pages_about_path %></li>
          <li><%= link_to "Projects", pages_projects_path %></li>
          <li><a href="#">Web Development</a></li>
          <li><a href="#">Contact</a></li>
        </div>
      </ul>
    </div>
    <%= yield %>

    <%= javascript_pack_tag "application" %>    <!-- from app/javascript/packs/application.js -->
  </body>
</html>
